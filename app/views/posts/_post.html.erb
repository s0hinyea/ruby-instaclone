<div id="<%= dom_id post %>">
  <div>
    
    <%= post.title %>
  </div>

  <div>
    
    <%= post.description %>
  </div>

  <p>
  <%= link_to post.user.username, post.user  %>
  </p>

  <div>
    <%= post.keywords %>
  </div>

  <div>
    
    <% post.images.each do |image| %>
      <%= image_tag(image, style: "object-fit: contain; width: 200px; height: 200px; ") %>
      <% end %> 
  </div>

</div>
